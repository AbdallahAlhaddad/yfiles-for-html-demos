{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "removeComments": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "dom.iterable",
      "es2017",
      "dom"
    ],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "loading/snowpack",
    "loading/vite",
    "loading/webworker-umd",
    "loading/webworker-webpack",
    "loading/wmr",
    "node_modules",
    "toolkit/angular",
    "toolkit/dojo",
    "toolkit/preact",
    "toolkit/react-typescript",
    "toolkit/svelte",
    "toolkit/vue2-cli-typescript",
    "toolkit/vue3-cli-typescript"
  ]
}
